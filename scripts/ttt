#!/bin/bash

CWD=`dirname $0`

begin=$1
step=$2
lineup=MSSS

out=out/results
rm -rf $out
mkdir -p $out

seed=$begin
for map in DEFAULT CHEESER MAP01 MAP02 MAP03
do
    file=$out/$map-$lineup.out
    end=`expr $seed + $step - 1`
    $CWD/mt $seed $end $map $lineup >$file
    echo "$map $lineup"
    java -ea -cp lib/kotlinc/lib/kotlin-runtime.jar:$CWD/kt-analyzer analyzer.AnalyzerPackage $file
    seed=`expr $end + 1`
done
